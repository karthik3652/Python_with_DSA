class Computer:
    def __init__(self, brand, ram, rom):
        self.brand = brand
        self.ram = ram
        self.rom = rom
        
    def info(self):
        print(self.brand, self.ram, self.rom)
        
comp1 = Computer('dell', '16gb', '1TB')
comp2 = Computer('hp', '8gb', '512gb')

comp1.info()
comp2.info()
